# Kalshi API Credentials (RSA-PSS authentication)
KALSHI_KEY_ID=your_kalshi_key_id
KALSHI_PRIVATE_KEY=your_base64_encoded_private_key

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
